<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00dafc"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="tsventine.stamat.Main2Activity">

    <SeekBar
        android:id="@+id/servo1"
        android:layout_width="274dp"
        android:layout_height="62dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/output2"
        android:layout_marginBottom="24dp"
        android:max="75"
        android:maxHeight="20dp"
        android:maxWidth="100dp"
        android:minHeight="20dp"
        android:minWidth="200dp"
        android:progress="25"
        android:progressDrawable="@color/accent_material_dark"
        android:progressTint="#b4252629"
        android:secondaryProgressTint="@color/accent_material_light"
        android:splitTrack="false"
        android:thumb="@android:drawable/btn_star_big_on" />

    <Button
        android:id="@+id/back"
        android:layout_width="121dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="44dp"
        android:layout_marginTop="24dp"
        android:text="Back" />

    <SeekBar
        android:id="@+id/servo2"
        android:layout_width="147dp"
        android:layout_height="63dp"
        android:layout_alignEnd="@+id/back"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="26dp"
        android:max="110"
        android:maxHeight="20dp"
        android:maxWidth="100dp"
        android:minHeight="20dp"
        android:minWidth="200dp"
        android:progress="55"
        android:progressDrawable="@color/accent_material_dark"
        android:progressTint="#b4252629"
        android:secondaryProgressTint="@color/accent_material_light"
        android:splitTrack="false"
        android:thumb="@android:drawable/btn_star_big_on" />

    <TextView
        android:id="@+id/output2"
        android:layout_width="72dp"
        android:layout_height="43dp"
        android:layout_alignStart="@+id/servo3"
        android:layout_alignTop="@+id/servo2"
        android:text="Command" />

    <SeekBar
        android:id="@+id/servo3"
        android:layout_width="274dp"
        android:layout_height="62dp"
        android:layout_centerVertical="true"
        android:layout_toEndOf="@+id/back"
        android:max="110"
        android:maxHeight="20dp"
        android:maxWidth="100dp"
        android:minHeight="20dp"
        android:minWidth="200dp"
        android:progress="55"
        android:progressDrawable="@color/accent_material_dark"
        android:progressTint="#b4252629"
        android:secondaryProgressTint="@color/accent_material_light"
        android:splitTrack="false"
        android:thumb="@android:drawable/btn_star_big_on" />

    <WebView
        android:id="@+id/webView"
        android:layout_width="177dp"
        android:layout_height="119dp"
        android:layout_alignEnd="@+id/servo2"
        android:layout_alignParentTop="true"
        android:visibility="visible" />

    <SeekBar
        android:id="@+id/servo6"
        android:layout_width="146dp"
        android:layout_height="62dp"
        android:layout_alignEnd="@+id/back"
        android:layout_centerVertical="true"
        android:max="195"
        android:maxHeight="20dp"
        android:maxWidth="100dp"
        android:minHeight="20dp"
        android:minWidth="200dp"
        android:progress="97"
        android:progressDrawable="@color/accent_material_dark"
        android:progressTint="#b4252629"
        android:secondaryProgressTint="@color/accent_material_light"
        android:splitTrack="false"
        android:thumb="@android:drawable/btn_star_big_on" />

    <SeekBar
        android:id="@+id/servo2"
        android:layout_width="352dp"
        android:layout_height="0dp"
        android:layout_above="@+id/back"
        android:layout_alignStart="@+id/back"
        android:layout_marginBottom="0dp"
        android:layout_marginStart="208dp"
        android:max="110"
        android:maxHeight="20dp"
        android:maxWidth="100dp"
        android:minHeight="20dp"
        android:minWidth="200dp"
        android:progress="10"
        android:progressDrawable="@color/accent_material_dark"
        android:progressTint="#b4252629"
        android:secondaryProgressTint="@color/accent_material_light"
        android:splitTrack="false"
        android:thumb="@android:drawable/btn_star_big_on" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/servo2"
        android:layout_alignStart="@+id/back"
        android:text="Lower Hand" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/servo6"
        android:layout_alignStart="@+id/back"
        android:text="Upper Hand" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="71dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/back"
        android:layout_toEndOf="@+id/back"
        android:text="Camera" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/servo3"
        android:layout_centerHorizontal="true"
        android:text="Hand" />

</RelativeLayout>
